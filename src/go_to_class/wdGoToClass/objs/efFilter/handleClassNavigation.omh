If pSystemKey=kUp
	If ilMethods.$line>1
		Calculate ilMethods.$line as ilMethods.$line-1
		Redraw {cgMethods}
	End If
	Quit method kTrue
End If

If pSystemKey=kDown
	If ilMethods.$line<ilMethods.$linecount
		Calculate ilMethods.$line as ilMethods.$line+1
		Redraw {cgMethods}
	Else
	End If
	Quit method kTrue
End If